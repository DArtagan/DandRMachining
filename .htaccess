# prevent viewing of a github.php file
<files github.php>
 order deny,allow
 deny from all
 allow from github.com
</files>


# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /projects/DandRMachining/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /projects/DandRMachining/index.php [L]
</IfModule>

# END WordPress

